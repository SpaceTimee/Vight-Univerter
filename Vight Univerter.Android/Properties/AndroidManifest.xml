<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="010000" android:versionName="1.0.1" package="SpaceTime.Vight.Univerter" android:installLocation="auto">
	<uses-sdk android:minSdkVersion="22" android:targetSdkVersion="30" />
	<application android:label="Vight Univerter" android:theme="@style/MainTheme" android:icon="@mipmap/Vight_Univerter_Logo"></application>
	<queries>
		<intent>
			<action android:name="android.intent.action.VIEW" />
			<data android:scheme="http" />
		</intent>
		<intent>
			<action android:name="android.intent.action.VIEW" />
